{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "pwd": "osc99",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .Shell }}",
          "foreground": "#0077c2",
          "background": "#000000",
          "type": "shell",
          "style": "accordion"
        },
        {
          "properties": {
            "cache_duration": "none",
            "root_icon": "\uf292"
          },
          "template": "<parentBackground>\ue0b0</> \uf0e7",
          "foreground": "#f75f5f",
          "background": "#000000",
          "type": "root",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_separator_icon": "/",
            "max_depth": 1,
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} ",
          "foreground": "#e95378",
          "powerline_symbol": "\ue0b0",
          "background": "#000000",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }} {{ end }} ",
          "foreground": "#29912c",
          "powerline_symbol": "\ue0b0",
          "background": "#000000",
          "type": "git",
          "style": "powerline",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFFB38{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#c5e478{{ end }}",
            "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
            "{{ if gt .Behind 0 }#C792EA{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "style": "austin",
            "threshold": 500
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " <#9b0af5>\ueba2</> {{ .FormattedMs }} ",
          "foreground": "#9b0af5",
          "background": "#000000",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_package_manager": true,
            "fetch_version": true,
            "npm_icon": "<#cc3a3a>\ue71e</>",
            "pnpm_icon": "<#FFA640>\ue71e</>",
            "yarn_icon": "<#66016D>\ue6a7</>"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Major }}.{{ .Minor }}",
          "foreground": "#3C873A",
          "background": "#000000",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue641 {{ .CurrentDate | date .Format }}",
          "foreground": "#0077c2",
          "background": "#000000",
          "type": "time",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \udb81\ude0d ",
          "foreground": "#0077c2",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
